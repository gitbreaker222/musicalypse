{
  "name": "Musicalypse",
  "description": "An HTML5 powered mp3 player.",
  "version": "0.1.0",
  "author": "Thomas Gambet <thomas.gambet@gmail.com> (https://github.com/tgambet/)",
  "license": "MIT",
  "main": "target/electron/dist/main.js",
  "scripts": {
    "ng": "ng",
    "ng:serve": "ng serve",
    "ng:serve:electron": "ng serve --environment=electron",
    "ng:build": "ng build",
    "ng:build:prod": "ng build --prod --aot",
    "ng:build:electron": "ng build --env=electron --aot",
    "ng:test": "ng test",
    "ng:lint": "ng lint",
    "ng:e2e": "ng e2e",
    "sbt:run": "sbt run",
    "sbt:clean": "sbt clean",
    "sbt:compile": "sbt compile",
    "sbt:stage": "sbt stage",
    "web:run": "npm-run-all ng:build:prod sbt:run",
    "web:serve": "npm-run-all --parallel ng:serve sbt:run",
    "web:serve:electron": "npm-run-all --parallel ng:serve:electron sbt:run",
    "web:build": "npm-run-all ng:build:prod sbt:compile",
    "web:stage": "npm-run-all ng:build:prod sbt:stage",
    "electron:run": "npm-run-all electron:build && electron .",
    "electron:run:dev": "npm run electron:tsc && electron .",
    "electron:serve": "npm run electron:tsc && npm-run-all --parallel web:serve:electron electron:serve_",
    "electron:serve_": "wait-on http-get://localhost:4200 && electron . --serve",
    "electron:build": "npm-run-all --parallel electron:tsc ng:build:electron sbt:stage",
    "electron:tsc": "tsc --project electron",
    "electron:stage": "",
    "electron:windows": "electron-builder build --windows",
    "postinstall": "electron-builder install-app-deps",
    "heroku-postbuild": "npm run ng:build:prod"
  },
  "private": true,
  "dependencies": {
    "wait-on": "^2.1.0"
  },
  "devDependencies": {
    "@angular/animations": "^5.2.9",
    "@angular/cdk": "^5.2.4",
    "@angular/cli": "^1.7.3",
    "@angular/common": "^5.2.9",
    "@angular/compiler": "^5.2.9",
    "@angular/compiler-cli": "^5.2.9",
    "@angular/core": "^5.2.9",
    "@angular/forms": "^5.2.9",
    "@angular/http": "^5.2.9",
    "@angular/language-service": "^5.2.9",
    "@angular/material": "^5.2.4",
    "@angular/platform-browser": "^5.2.9",
    "@angular/platform-browser-dynamic": "^5.2.9",
    "@angular/router": "^5.2.9",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/lodash": "^4.14.104",
    "@types/node": "~6.0.60",
    "codelyzer": "~4.0.1",
    "core-js": "^2.5.3",
    "dotenv": "^5.0.1",
    "electron": "^2.0.0-beta.6",
    "electron-builder": "^20.8.1",
    "electron-reload": "^1.2.2",
    "hammerjs": "^2.0.8",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lodash": "^4.17.5",
    "minireset.css": "0.0.3",
    "npm-run-all": "^4.1.2",
    "protractor": "~5.1.2",
    "rxjs": "^5.5.6",
    "ts-node": "~3.2.0",
    "tslint": "~5.8.0",
    "typescript": "~2.6.2",
    "zone.js": "^0.8.20"
  },
  "engines": {
    "node": "8.11.1",
    "npm": "5.7.1"
  }
}
